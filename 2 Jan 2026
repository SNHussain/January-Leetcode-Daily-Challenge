class Solution {
public:
    int repeatedNTimes(vector<int>& nums) {
        int n = nums.size() / 2;

        unordered_map<int , int> freq; //{element , count}

        for(int it: nums){
            freq[it]++;

            if(freq[it] == n){
                return it;
            }
        } 

        return -1;
    }
};
